{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Azure": {
    "OpenAI": {
      "Endpoint": "https://your-openai-resource.openai.azure.com/",
      "DeploymentName": "chat-completions",
      "ApiVersion": "2024-02-15-preview"
    },
    "Search": {
      "Endpoint": "https://your-search-resource.search.windows.net/",
      "IndexName": "rag-index"
    },
    "Storage": {
      "AccountName": "yourstorageaccount"
    }
  },
  "Stripe": {
    "PublishableKey": "pk_test_placeholder",
    "SecretKey": "sk_test_placeholder",
    "WebhookSecret": "whsec_placeholder"
  },
  "UsageLimits": {
    "FreeMessagesPerDay": 25,
    "ProMessagesPerDay": 250
  },
  "Authentication": {
    "Authority": "https://your-tenant.b2clogin.com/tenant.onmicrosoft.com/v2.0",
    "ClientId": "00000000-0000-0000-0000-000000000000",
    "ClientSecret": "your-client-secret",
    "CallbackPath": "/signin-oidc",
    "SignedOutCallbackPath": "/signout-callback-oidc"
  },
  "Cosmos": {
    "AccountEndpoint": "https://your-cosmos-account.documents.azure.com:443/",
    "Database": "appdb",
    "Container": "items"
  }
}
